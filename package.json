{
  "name": "afromerkto",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">= 10",
    "yarn": "Please use NPM"
  },
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "nodemon -x concurrently npm:dev:*",
    "build": "rimraf dist && tsc && npm run copy",
    "build:admin": "rimraf admin-ui && npx ts-node ./src/compile-admin-ui.ts",
    "start:server": "node ./dist/src/index.js",
    "start:worker": "node ./dist/src/index-worker.js",
    "start": "concurrently npm:start:*",
    "codegen": "graphql-codegen --config codegen.ts",
    "compile-admin-ui": "ts-node ./src/compile-admin-ui.ts",
    "copy": "npx copyfiles -u 1 'admin-ui/dist/**/*' dist",
    "lint:check": "npx prettier --check . && DEBUG=eslint:cli-engine npx eslint .",
    "lint:fix": "DEBUG=eslint:cli-engine npx prettier --write . && npx eslint --fix .",
    "commitlint-check": "npx --no-install commitlint --edit"
  },
  "dependencies": {
    "@nestjs/schedule": "^4.1.0",
    "@vendure/admin-ui-plugin": "3.0.0",
    "@vendure/asset-server-plugin": "3.0.0",
    "@vendure/core": "3.0.0",
    "@vendure/email-plugin": "3.0.0",
    "@vendure/harden-plugin": "^3.0.0",
    "@vendure/testing": "^3.0.0",
    "@vendure/ui-devkit": "^3.0.0",
    "axios": "^1.7.4",
    "chart.js": "4.3.0",
    "copyfiles": "^2.4.1",
    "cron": "^3.1.7",
    "dotenv": "16.4.5",
    "jspdf": "^2.5.1",
    "mysql": "2.18.1",
    "nodemon": "^3.1.4",
    "pdfmake": "^0.2.10",
    "vitest": "^2.0.5",
    "write-excel-file": "^2.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.9.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@vendure/cli": "^3.0.0",
    "concurrently": "8.2.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.1.0"
  }
}
